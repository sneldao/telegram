# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
ENVIRONMENT=production

# Connection Settings
TELEGRAM_READ_TIMEOUT=30
TELEGRAM_CONNECT_TIMEOUT=30

# API Keys (Optional)
COINMARKETCAP_API_KEY=your_coinmarketcap_key_here
TWITTER_CONSUMER_KEY=your_twitter_key_here
TWITTER_CONSUMER_SECRET=your_twitter_secret_here
TWITTER_ACCESS_TOKEN=your_twitter_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_token_secret_here
CRYPTOPANIC_API_KEY=your_cryptopanic_key_here

# SSL Configuration (Will be managed by Canine)
SSL_CERT_PATH=/app/conf/ssl/webhook.pem
SSL_KEY_PATH=/app/conf/ssl/webhook.key 